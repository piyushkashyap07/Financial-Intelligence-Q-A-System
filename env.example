# SEC Filing Data Processing System - Environment Variables
# Copy this file to .env and fill in your actual values

# Pinecone Vector Database Configuration
PINECONE_API_KEY=your_pinecone_api_key_here
PINECONE_CLOUD=aws
PINECONE_REGION=us-east-1

# Google AI Configuration (for Gemini embeddings and LLM)
GOOGLE_API_KEY=your_google_api_key_here
GEMINI_MODEL_NAME=models/embedding-001

# SEC Scraper Configuration
SEC_USER_AGENT=YourCompany contact@yourcompany.com

# Application Configuration
STREAMLIT_SERVER_PORT=8501
STREAMLIT_SERVER_ADDRESS=0.0.0.0

# Optional: Redis for caching (uncomment if using Redis)
# REDIS_URL=redis://localhost:6379

# Optional: Logging level
LOG_LEVEL=INFO 